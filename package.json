{
  "name": "@uiw/react-codepen",
  "version": "1.0.3",
  "description": "A React component is provided that allows you to programmatically generate codepen projects from code samples on the fly.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "homepage": "https://uiwjs.github.io/react-codepen",
  "scripts": {
    "prepare": "npm run build",
    "doc": "kkt build --app-src ./website",
    "start": "kkt start --app-src ./website",
    "watch": "tsbb watch src/*.tsx --useBabel",
    "build": "tsbb build src/*.tsx --useBabel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-codepen.git"
  },
  "author": "Kenny Wang <wowohoo@qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "@babel/runtime": ">=7.10.0"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "devDependencies": {
    "@kkt/less-modules": "~7.4.9",
    "@kkt/raw-modules": "~7.4.9",
    "@kkt/scope-plugin-options": "~7.4.9",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@uiw/react-github-corners": "~1.5.3",
    "@uiw/react-markdown-preview": "~4.1.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "kkt": "~7.4.9",
    "tsbb": "^4.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4"
  }
}
